# Consultas SQL para el proceso de reverso
select_microcredit=SELECT * FROM KMIC.T_KMIC_MICROCREDIT_CONTRACT WHERE G_CONTRACT_ID = :contractId;
update_microcredit=UPDATE KMIC.T_KMIC_MICROCREDIT_CONTRACT SET ... WHERE G_CONTRACT_ID = :contractId;
list_dispositions=SELECT * FROM KMIC.T_KMIC_MCRCR_CONT_DISPOSITION WHERE G_CONTRACT_ID = :contractId;
update_cont_diposition=UPDATE KMIC.T_KMIC_MCRCR_CONT_DISPOSITION SET ... WHERE G_CONTRACT_ID = :contractId;
list_dspn=SELECT * FROM KMIC.T_KMIC_MCRCR_DSPN_AMORT WHERE G_CONTRACT_ID = :contractId;
update_dspn=UPDATE KMIC.T_KMIC_MCRCR_DSPN_AMORT SET ... WHERE G_CONTRACT_ID = :contractId;
list_amort_cond=SELECT * FROM KMIC.T_KMIC_AMORTIZATION_CONDITION WHERE G_CONTRACT_ID = :contractId;
update_amort_cond=UPDATE KMIC.T_KMIC_AMORTIZATION_CONDITION SET ... WHERE G_CONTRACT_ID = :contractId;
